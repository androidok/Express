<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
	
	<struts>
		<package name="admin" namespace="/admin" extends="struts-default">
			<action name="*_*" class="com.express.admin.action.{1}Action" method="{2}">
				<result name="login_success">/admin/login_success.jsp</result>
				<result name="login_failure">/admin/login.jsp</result>
				<result name="input">/admin/login.jsp</result>
				<result name="finish">/admin/allUser.jsp</result>
				<result name="order">/admin/order.jsp</result>
			</action>
		</package>	
	
	</struts>